# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/unbuilt

# JWT Authentication (Required for production)
JWT_ACCESS_SECRET=your_jwt_access_secret_at_least_32_characters_long
JWT_REFRESH_SECRET=your_jwt_refresh_secret_at_least_32_characters_long_and_different
JWT_ACCESS_EXPIRY=15m
JWT_REFRESH_EXPIRY=7d

# Security Configuration
CORS_ORIGIN=http://localhost:5000
COOKIE_SECRET=your_cookie_secret_at_least_32_characters_long
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# Database Connection Pool
DB_MAX_CONNECTIONS=10

# AI Service Configuration (Optional)
GEMINI_API_KEY=your_gemini_api_key_here
XAI_API_KEY=your_xai_api_key_here
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# Email Service (Optional)
SENDGRID_API_KEY=your_sendgrid_api_key_here

# Payment Processing (Optional)
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# OAuth Providers (Optional)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here

# Replit-specific (Optional - for Replit deployment)
REPLIT_DOMAINS=your_replit_domains_here
ISSUER_URL=https://replit.com/oidc
REPL_ID=your_repl_id_here
SESSION_SECRET=your_session_secret_here

# Demo User Configuration (Development only)
# Set these to create a demo user for development/testing
# Leave empty in production to disable demo user creation
DEMO_USER_EMAIL=your_demo_email@example.com
DEMO_USER_PASSWORD=your_secure_demo_password_min_8_chars

# Environment
NODE_ENV=development